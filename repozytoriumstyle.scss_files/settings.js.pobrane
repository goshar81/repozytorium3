App.Views.Settings=Backbone.View.extend({events:{"click .toggle-connect":"toggleConnect"},toggleConnect:function(a){var b=$(a.currentTarget);if(!b.hasClass("main-account")){var c=b.find("span:visible").data("url");c&&(b.hasClass("connected")?this.toggleConnectAjax(c).done(function(){analytics.track("account_"+b.find("span:visible").data("name")+"_unlinked"),b.toggleClass("connected not-connected")}):(analytics.track("account_"+b.find("span:visible").data("name")+"_linked"),window.location.href=c))}},toggleConnectAjax:function(a){return $.ajax({type:"POST",url:a})}});